# Coverage configuration

[run]
source = scripts,analysis
omit = 
    */tests/*
    */test_*.py
    */__pycache__/*
    */site-packages/*
    */.venv/*
    */venv/*

branch = True

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
title = TriGuard Insurance Test Coverage Report

[xml]
output = coverage.xml

